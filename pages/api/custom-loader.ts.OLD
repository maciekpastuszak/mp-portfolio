export default function cloudinaryLoader({
    src,
    width,
    quality,
  }: {
    src: string
    width: number
    quality?: number
  }) {
    const params = ['f_auto', 'c_limit', `w_${width}`, `q_${quality || 'auto'}`]
    return `  https://res.cloudinary.com/dqcadja0y/image/upload/v1695025731/maciejpastuszak/screen_hoobank_kvqas4.jpg===/${params.join(
      ','
    )}${src}`
  }